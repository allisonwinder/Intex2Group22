@{
}

@model MummiesViewModel

<h1>Mummies</h1>
@foreach (Burialmain b in Model.Burialmains)
{
  <ul>
    <li>Hair: @b.Hair</li>
    <li>Sex: @b.Sex</li>
    <li>Length: @b.Length</li>
    <li>Date of Excavation: @b.Dateofexcavation</li>
</ul>  
<br />
}


<div page-blah="@Model.PageInfo" page-action="allMummies" page-classes-enabled="true"
     page-class="btn" page-class-normal="btn-outline-dark"
     page-class-selected="btn-secondary" class="btn-group pull-right m-1"></div>

